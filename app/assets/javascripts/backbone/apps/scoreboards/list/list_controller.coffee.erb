@App.module "ScoreboardsApp.List", (List, App, Backbone, Marionette, $, _) ->

  List.Controller =
    show: ->
      si = App.request 'entities:scoreboardInfo'
      si.set 'view', '<%= I18n.t "scoreboard.header.view_selector.options.list" %>'

      view = new List.View
      App.mainRegion.show view
