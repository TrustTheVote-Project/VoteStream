<div id="filter-bar">
  <div class="row">
    <div class="col-xs-6">
      <a href="#" onclick="return false;" id="js-view-back">&laquo; Return to Previous Screen</a>
      <br/>    
      <h3>Compare Maps</h3>
    </div>
    <div class="col-xs-6">
      <a href="#metadata"  id="js-election-metadata">View Election Metadata &raquo;</a>
      <label id="percent-reporting"><%%= gon.percentReporting %>&nbsp;&nbsp;</label>
    </div>
  </div>
  <div class="row-fluid">
      <div class="col-xs-12 ">
        <%% for map in @maps(): %>
          <%%= map.getName() %>
        <%% end %>
      </div>
  </div>
</div>
<div id="content">
  <div id="map-comparison">
    <div class="row">
      <%% for map in @maps(): %>
        <div class="col-xs-6 map-comparison-item">
          <h4>
            <a href="<%%= map.url %>"><%%= map.getName() %></a>
          </h4>
          <p id="map-description-region-<%%= map.id %>">loading</p>
          <div id="map-region-<%%= map.id %>" class="map-comparison-item-map"></div>
        </div>
      <%% end %>    
    </div>
  </div>
</div>